{
  "name": "pia",
  "version": "1.6.5",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod --build-optimizer",
    "clean": "rimraf ./dist/ ./node_modules",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.4",
