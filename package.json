{
  "name": "pia",
  "version": "1.6.4",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prod": "ng build --prod --build-optimizer --sourcemaps",
    "clean": "rimraf ./dist/ ./node_modules",
    "format:write": "prettier {src,e2e}/**/*.{ts,json,md,scss} --write",
    "format:test": "prettier {src,e2e}/**/*.{ts,json,md,scss} --list-different",
    "compodoc": "compodoc -p src/tsconfig.app.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run lint"
    }
  },
  "lint-staged": {
    "{src,e2e}/**/*.{ts,json,md,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.3",
    "@angular/common": "^4.4.3",
    "@angular/compiler": "^4.4.3",
    "@angular/core": "^4.4.3",
    "@angular/forms": "^4.4.3",
    "@angular/http": "^4.4.3",
    "@angular/platform-browser": "^4.4.3",
    "@angular/platform-browser-dynamic": "^4.4.3",
    "@angular/router": "^4.4.3",
    "@ngx-translate/core": "^8.0.0",
    "@ngx-translate/http-loader": "^1.0.2",
    "angular2-csv": "0.2.5",
    "core-js": "^2.4.1",
    "d3": "^4.10.2",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.3.1",
    "html2canvas": "^1.0.0-alpha.10",
    "ngx-chips": "~1.5.11",
    "normalize.css": "^7.0.0",
    "rxjs": "^5.1.0",
    "save-svg-as-png": "^1.4.6",
    "tinymce": "^4.6.7",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^4.4.3",
    "@angular/language-service": "^4.4.3",
    "@compodoc/compodoc": "^1.1.6",
    "@types/jasmine": "^3.3.0",
    "@types/jasminewd2": "^2.0.6",
    "@types/node": "^10.12.10",
    "codelyzer": "~4.5.0",
    "hoek": "^6.0.3",
    "husky": "^1.2.1",
    "jasmine": "~3.3.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "karma-spec-reporter": "~0.0.32",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "protractor": "~5.1.2",
    "rimraf": "^2.6.2",
    "source-map-explorer": "^1.5.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.11.0",
    "typescript": "^2.3.4"
  }
}
