    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@types/pdfjs-dist": "^0.1.2",
    "angular2-csv": "~0.2.9",
    "core-js": "~2.6.5",
    "d3": "~5.9.1",
    "foundation-sites": "^6.5.3",
    "html2canvas": "~1.0.0-alpha.12",
